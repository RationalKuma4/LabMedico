@model IEnumerable<LabMedico.Models.Historico>

@{
    ViewBag.Title = "Historicos";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@ViewBag.Title</h2>

<p>
    @Html.ActionLink("Registrar", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Citas.HoraAplicacion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaRegistro)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Monto)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Citas.HoraAplicacion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaRegistro)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Monto)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.HistoricoId }) |
            @Html.ActionLink("Detalle", "Details", new { id=item.HistoricoId }) |
            @Html.ActionLink("Borrar", "Delete", new { id=item.HistoricoId })
        </td>
    </tr>
}

</table>
