@model IEnumerable<LabMedico.Models.Analisis>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table table-striped">
    <tr>
        @*<th>
                @Html.DisplayNameFor(model => model.Estudios.Nombre)
            </th>*@
        <th>
            @Html.DisplayNameFor(model => model.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Requisitos)
        </th>
        @*<th>
                @Html.DisplayNameFor(model => model.Estatus)
            </th>*@
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            @*<td>
                    @Html.DisplayFor(modelItem => item.Estudios.Nombre)
                </td>*@
            <td>
                @Html.DisplayFor(modelItem => item.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Descripcion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Requisitos)
            </td>
            @*<td>
                    @Html.DisplayFor(modelItem => item.Estatus)
                </td>*@
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.AnalisisId }) |
                @Html.ActionLink("Details", "Details", new { id = item.AnalisisId }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.AnalisisId })
            </td>
        </tr>
    }

</table>
